@{
    ViewData["Title"] = "Home Page";
 
}

<div class="text-center">
    <h1 class="display-4">Captura de datos</h1>

    <div class="mb-3">
        <label class="form-label">Name </label>
        <input type="text" class="form-control" id="name" placeholder="">
    </div>

    <div class="mb-3">
        <label class="form-label">Last Name </label>
        <input type="email" class="form-control" id="Lastname" placeholder="">
    </div>


    <div class="mb-3">
        <label class="form-label">Number </label>
        <input type="number" class="form-control" id="number" placeholder="00-00-00-00-00">
    </div>


    <div class="mb-3">
        <label class="form-label">Email </label>
        <input type="email" class="form-control" id="mail" placeholder="name@example.com">
    </div>

    <div class="mb-3">
        <label class="form-label">Address </label>
        <input type="text" class="form-control" id="adress" placeholder="">
    </div>

    <div class="mb-3">
        <label class="form-label">Hability </label>
        <textarea class="form-control" id="habilidad" rows="3"></textarea>
    </div>
    <div class="mb-3">
        <label class="form-label">Hability 2 </label>
        <textarea class="form-control" id="habilidad2" rows="3"></textarea>
    </div>

    <button type="button" class="btn btn-success">Insertar</button>




</div>
